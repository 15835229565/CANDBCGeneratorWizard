{
    "ComStack": {
        "Template": [
            {
                "SearchingElement": {
                    "SHORT-NAME": "Com"
                },
                "SearchingContainer": {
                    "SHORT-NAME": "ComConfig"
                },
                "SearchingSubContainer": {
                    "DEFINITION-REF": "^[\\S]*ComSignal$"
                },
                "ParametersTemplate": {
                    "^[\\S]*ComTransferProperty$": {
                        "__RX__": "PENDING"
                    },
                    "^[\\S]*ComFirstTimeout$": {
                        "__RX__": "{__TRANSFER_TIME_LOOP__}*10"
                    },
                    "^[\\S]*ComTimeout$": {
                        "__RX__": "{__TRANSFER_TIME_LOOP__}*10"
                    },
                    "^[\\S]*ComNotification$": {
                        "__RX__": "Rte_COMCbk_{__ARXML__.__SHORT_NAME__}"
                    },
                    "^[\\S]*ComTimeoutNotification$": {
                        "__RX__": "Rte_ComCbkTOut_{__ARXML__.__SHORT_NAME__}"
                    },
                    "^[\\S]*ComErrorNotification$": null
                }
            }
        ]
    }
}